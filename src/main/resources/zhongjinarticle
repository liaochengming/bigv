        function toggleFun(obj){
            var f = $(obj).attr('rel');
            if(f == '1'){
                $('#archiveDiv').css({ 'height': ''});
                $(obj).html('点击收起↑');
                $(obj).attr('rel','2');
            }else{
                $('#archiveDiv').css({ 'height': '300px'});
                $(obj).html('点击展开↓');
                $(obj).attr('rel','1');
            }
        }
